#include <iostream>
#include <conio.h>
using namespace std;
int main(void)
{
	int det,i,j;
	int ax[2][2];	
          for (i=1; i<=2; i++){
          for (j=1; j<=2; j++){
     cout << " ["<<i<<"]["<<j<<"] : "; cin >> ax[i][j];
              }
           }
     cout << endl;
          for (i=1; i<=2; i++){
          for (j=1; j<=2; j++){
     cout << "      "<<ax[i][j]<<"      ";
              }
     cout << "\n";
          }       
}
